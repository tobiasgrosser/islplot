        "\t\t\tface = triangles[ i ];\n",
        "\n",
        "\t\t\tfor ( f = 0; f < 3; f ++ ) {\n",
        "\n",
        "\t\t\t\tkey = face[ f ].x + \":\" + face[ f ].y;\n",
        "\n",
        "\t\t\t\tindex = allPointsMap[ key ];\n",
        "\n",
        "\t\t\t\tif ( index !== undefined ) {\n",
