        "\t\t\t\t}\n",
        "\n",
        "\t\t\t\tif ( textureJSON.offset ) {\n",
        "\n",
        "\t\t\t\t\ttexture.offset.set( textureJSON.offset[ 0 ], textureJSON.offset[ 1 ] );\n",
        "\n",
        "\t\t\t\t}\n",
        "\n",
        "\t\t\t\t// handle wrap after repeat so that default repeat can be overriden\n",
