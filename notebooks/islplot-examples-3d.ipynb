        "\t\t\t}\n",
        "\n",
        "\t\t};\n",
        "\n",
        "\t\t// first go synchronous elements\n",
        "\n",
        "\t\t// fogs\n",
        "\n",
        "\t\tvar fogID, fogJSON;\n",
        "\n",
