        "\n",
        "\t\t\tfor( var k = 0; k < data.hierarchy[ h ].keys.length; k ++ ) {\n",
        "\n",
        "\t\t\t\t// remove minus times\n",
        "\n",
        "\t\t\t\tif( data.hierarchy[ h ].keys[ k ].time < 0 )\n",
