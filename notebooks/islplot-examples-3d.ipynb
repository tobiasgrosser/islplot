        "\n",
        "\t\t}\n",
        "\n",
        "\t\t// materials\n",
        "\n",
        "\t\tvar matID, matJSON;\n",
        "\t\tvar parID;\n",
        "\n",
        "\t\tfor ( matID in data.materials ) {\n",
        "\n",
        "\t\t\tmatJSON = data.materials[ matID ];\n",
        "\n",
        "\t\t\tfor ( parID in matJSON.parameters ) {\n",
        "\n",
        "\t\t\t\tif ( parID === \"envMap\" || parID === \"map\" || parID === \"lightMap\" || parID === \"bumpMap\" ) {\n",
        "\n",
