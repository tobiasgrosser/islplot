        "\t\t\t\treturn null;\n",
        "\n",
        "\t\t\t}\n",
        "\n",
        "\t\t} else {\n",
        "\n",
        "\t\t\t// todo: add simple tween library\n",
        "\n",
        "\t\t}\n",
        "\n",
        "\t};\n",
        "\n",
        "\t//--- parse ---\n",
        "\n",
        "\tthat.parse = function( root ) {\n",
