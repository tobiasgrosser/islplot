        "\n",
        "\t\t\"lightMap\" : { type: \"t\", value: null },\n",
        "\t\t\"specularMap\" : { type: \"t\", value: null },\n",
        "\n",
        "\t\t\"envMap\" : { type: \"t\", value: null },\n",
        "\t\t\"flipEnvMap\" : { type: \"f\", value: -1 },\n",
